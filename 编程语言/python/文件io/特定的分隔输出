>>> print('ACME', 50, 91.5)
ACME 50 91.5
>>> print('ACME', 50, 91.5, sep=',')
ACME,50,91.5
>>> print('ACME', 50, 91.5, sep=',', end='!!\n')
ACME,50,91.5!!
>>> print(','.join(['ACME','50','91.5']))
ACME,50,91.5
>>> row = ('ACME', 50, 91.5)
>>> print(','.join(str(x) for x in row))
ACME,50,91.5
>>> 
>>> print(*row, sep=',')
ACME,50,91.5
>>> 
