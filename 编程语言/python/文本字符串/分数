>>> from fractions import Fraction
>>> a = Fraction(5, 4)            
>>> b = Fraction(7, 16)
>>> a
Fraction(5, 4)
>>> b
Fraction(7, 16)
>>> a + b
Fraction(27, 16)
>>> a * b
Fraction(35, 64)
>>> c = a * b
>>> c.numerator
35
>>> c.denominator 
64
>>> float(c)
0.546875
>>> print(c.limit_denominator(8))
4/7
>>> x = 3.75
>>> y = Fraction(*x.as_integer_ratio())
>>> y
Fraction(15, 4)
>>> 
